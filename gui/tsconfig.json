{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "jsx": "react",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "moduleResolution": "node",
      "baseUrl": "./src",
      "paths": {
        "*": ["node_modules/*", "src/*"]
      }
    },
    "include": ["src"]
  }
  